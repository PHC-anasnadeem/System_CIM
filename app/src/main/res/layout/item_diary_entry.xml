<?xml version="1.0" encoding="utf-8"?>


<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp"
    app:cardCornerRadius="8dp"
    app:cardElevation="4dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">

        <TextView
            android:id="@+id/tvFinalID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Final ID"
            android:textColor="#000"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/tvOutletName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Outlet Name"
            android:textColor="#666"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvAqcFileNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AQC File No"
            android:textColor="#666"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvDiaryNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Diary No"
            android:textColor="#666"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvDistrict"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="District"
            android:textColor="#666"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvdeseal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="De-seal Date"
            android:textColor="#666"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvComments"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Comments"
            android:textColor="#666"
            android:textSize="14sp" />


    </LinearLayout>
</androidx.cardview.widget.CardView>




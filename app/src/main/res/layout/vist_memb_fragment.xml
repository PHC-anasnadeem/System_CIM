<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/container1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
    tools:context="com.phc.cim.Extra.Vist_Memb_Fragment">

        <FrameLayout
            android:id="@+id/content1"
            android:layout_width="match_parent"
            android:layout_height="438dp"
            android:layout_weight="1"
            android:background="@drawable/pages_bg">



            <Button
                android:id="@+id/Member"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:layout_marginBottom="100dp"
                android:layout_marginLeft="110dp"
                android:layout_marginRight="110dp"
                android:layout_marginTop="100dp"
                android:text="PHC Registration"
                android:background="@drawable/phc_member_btn" />


            <Button
                android:id="@+id/Visitor"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:layout_marginBottom="60dp"
                android:layout_marginLeft="110dp"
                android:layout_marginRight="110dp"
                android:layout_marginTop="140dp"
                android:text="Find NearBy HCEs"
                android:textColor="#ffffff"
                android:background="@drawable/phc_visitor_btn" />

            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/navigation"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="bottom"
                android:background="@drawable/pages_border"
                app:menu="@menu/navigation"
                app:itemTextColor="#ffffff"
                app:itemIconTint="#ffffff"
                />



        </FrameLayout>

    </LinearLayout>
